<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.0.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_0_0.dtd">

<liferay-portlet-app>
  <portlet>
    <portlet-name>ActivateAccount</portlet-name>
    <friendly-url-mapper-class>com.liferay.portal.kernel.portlet.DefaultFriendlyURLMapper</friendly-url-mapper-class>
    <friendly-url-mapping>aktivera</friendly-url-mapping>
    <friendly-url-routes>activation-friendly-url-routes.xml</friendly-url-routes>
    <action-url-redirect>true</action-url-redirect>
  </portlet>
  <portlet>
    <portlet-name>CreateAccount</portlet-name>
    <action-url-redirect>true</action-url-redirect>
  </portlet>
  <portlet>
    <portlet-name>ReinviteAccount</portlet-name>
    <action-url-redirect>true</action-url-redirect>
  </portlet>

</liferay-portlet-app>